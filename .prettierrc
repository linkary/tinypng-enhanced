{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 110,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "spaceBeforeFunctionParen": false,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "useTabs": false
}
